{
  "name": "web-final",
  "version": "1.0.0",
  "description": "Full stack ecommerce webpage",
  "main": "server.js",
  "scripts": {
    "client-install": "npm i --prefix client",
    "start": "node dist/server.js",
    "server": "nodemon dist/server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\" \"npm run watch\"",
    "watch": "tsc -w",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm i --prefix client && npm run build --prefix client"
  },
  "author": "Thanh Nguyen",
  "license": "MIT",
  "dependencies": {
    "apollo-server-express": "^3.6.7",
    "config": "^3.3.7",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "nodemailer": "^6.7.2",
    "pg": "^8.7.3",
    "stripe": "^8.198.0",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.3.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/connect-redis": "^0.0.18",
    "@types/nodemailer": "^6.4.4",
    "@types/uuid": "^8.3.4",
    "argon2": "^0.28.5",
    "concurrently": "^7.0.0",
    "connect-redis": "^6.1.3",
    "express-session": "^1.17.2",
    "ioredis": "^5.0.4",
    "nodemon": "^2.0.15",
    "typescript": "^4.6.3"
  }
}
